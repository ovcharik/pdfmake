{
  "name": "pdfmake",
  "version": "0.1.34",
  "description": "Client/server side PDF printing in pure JavaScript",
  "main": "src/printer.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "file-saver": "^1.3.3",
    "fontkit": "^1.0.0",
    "iconv-lite": "^0.4.19",
    "linebreak": "^0.3.0",
    "png-js": ">=0.1.0"
  },
  "devDependencies": {
    "base64-image-loader": "^1.2.1",
    "base64-loader": "^1.0.0",
    "brfs": "^1.4.3",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^1.12.7",
    "json-loader": "^0.5.7",
    "string-replace-webpack-plugin": "^0.1.3",
    "transform-loader": "^0.2.4",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.8.1",
    "webpack-stream": "^3.2.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:examples": "npm run build; cd ./examples; mkdir -p pdfs; rm -rf pdfs/*; echo \"\\nBuilding:\"; for file in *.js; do printf \"%50s, time (ms): \" $file; node $file; done;",
    "test": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bpampuch/pdfmake.git"
  },
  "keywords": [
    "pdf",
    "javascript",
    "printing",
    "layout"
  ],
  "author": "Bartek Pampuch <bartosz.pampuch@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpampuch/pdfmake/issues"
  },
  "homepage": "http://pdfmake.org",
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  }
}
